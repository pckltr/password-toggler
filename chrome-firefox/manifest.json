{
  "manifest_version": 3,
  "name": "Password Toggler - view typed passwords",
  "short_name": "passTogg",
  "version": "1.4",
  "description": "Adds button to show/hide password in input elements",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ]
}
